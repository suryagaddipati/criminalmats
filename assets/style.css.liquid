.nav-item.dropdown:hover { z-index:99999; }
.nav-item.dropdown:hover .sub-nav { display:block }
.nav-item.dropdown > a:after {
  content: ' ';
  display:-moz-inline-stack;
  display:inline-block;
  zoom:1;
  *display:inline;
