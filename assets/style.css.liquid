  padding:0;
  list-style-type:none;
  display:block;
  width:230px;
  position:absolute;
  left:50%;
  top:22px;
  margin-left:-115px;
  display:none;
  z-index:1337;
}

.sub-nav:after {
  content:'';
  display:block;
  top:17px;
  bottom:0;
  left:0;
  right:0;
  position:absolute;
  -webkit-border-radius:4px;
  -moz-border-radius:4px;
  border-radius:4px;
