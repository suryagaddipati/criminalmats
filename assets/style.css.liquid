}

.contact-form textarea.styled-input, .comment-form textarea.styled-input {
  min-width: 400px;
}

.styled-submit, .btn, .button {
  display:block;
  padding:11px 14px;
  background-color:{{ settings.global_button_color }};
  color:{{ settings.global_button_font_color }};
  font-weight:700;
  -webkit-border-radius:4px;
  -moz-border-radius:4px;
  border-radius:4px;
  border:none;
  outline:none;
  font-size:14px;
  position:relative;
  z-index:1;
  -webkit-text-stroke:0.1px;
