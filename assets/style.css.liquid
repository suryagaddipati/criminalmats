  margin:0 3px;
}

.pagination a { color:{{ settings.global_small_link_color }}; }

.pagination-ellipsis { margin:0 3px; }

.pagination a.active { color:{{settings.global_link_color}}; }
.pagination a.active:hover { 
  background-color:{{ settings.pagination_bg_hover_color }};
  border: 1px solid {{ settings.pagination_border_hover_color }};
}

.pagination span { color:{{ settings.global_text_font_color }}; }
                
        
/* ON SALE & COMPARE AT PRICE */

.on-sale { color:{{ settings.sale_price_color }} !important; }

.compare-at-price {
  font-style:normal;
  text-decoration:line-through;
  color:{{ settings.price_color }};
}
        
/* PRODUCT AND COLLECTION GRID */

.four-per-row, .three-per-row {
  display:block;
  float:left;
  width:191px;
  margin: 0 0 36px 40px;
}

.four-per-row {
  width:191px;
}

.three-per-row { 
  width:268px;
}

.coll-prod-caption {
  position:relative;
}

.coll-prod-buy.disabled {
  -moz-opacity:0.50;
  opacity:.50;
  filter:alpha(opacity=50);
}

.four-per-row:hover .coll-prod-buy.disabled, .three-per-row:hover .coll-prod-buy.disabled {
  background-color:{{ settings.global_button_color }};
}

.four-per-row:hover .coll-prod-buy, .three-per-row:hover .coll-prod-buy {
  background-color:{{ settings.global_button_color_hover }};
}

.four-per-row:hover .coll-prod-title, .three-per-row:hover .coll-prod-title {
  color:{{ settings.global_hover_color }};
}

.four-per-row:hover .sale-overlay, .three-per-row:hover .sale-overlay {
  opacity:1;
  z-index:1337;
}

.four-per-row .coll-image-wrap, .three-per-row .coll-image-wrap {
  overflow:hidden;
  text-align:center;
  position:relative;
  {% unless settings.prod_thumbnail_background_transparent %}
  background-color:{{ settings.prod_thumbnail_background }};
  {% endunless %}
}

.four-per-row .coll-image-wrap {
  width:191px;
  height:{{ 191.0 | divided_by: settings.product_img_w_to_h_ratio }}px;
}

.three-per-row .coll-image-wrap {
  width:268px;
  height:{{ 268.0 | divided_by: settings.product_img_w_to_h_ratio }}px;
}

.four-per-row .coll-image-wrap img, .three-per-row .coll-image-wrap img {
  opacity:0;
}

.four-per-row .coll-image-wrap img {
  max-width:191px;
}

.three-per-row .coll-image-wrap img {
  max-width:268px;
}

.four-per-row .coll-image-wrap a, .three-per-row .coll-image-wrap a { 
  display:block; 
}

.four-per-row .coll-prod-buy, .three-per-row .coll-prod-buy {
  background-color:{{ settings.global_button_color }};
  text-align:center;
  display:block;
  float:right;
  position:absolute;
  top:0;
  right:0;
  font-weight:700;
  font-size:13px;
  color:{{ settings.global_button_font_color }};
  font-family:{{ settings.text_font }};
  line-height:14px;
}

.four-per-row .coll-prod-buy {
  margin-top:16px;
  padding:5px 10px 6px;
  font-size:13px;
}

.three-per-row .coll-prod-buy  {
  margin-top:19px;
  padding:7px 12px 8px;
  font-size:14px;
}

@media screen and (-webkit-min-device-pixel-ratio:0) {
  .four-per-row .coll-prod-buy {
    padding:5px 10px 7px;
  }
}

@media screen and (-webkit-min-device-pixel-ratio:0) {
  .three-per-row .coll-prod-buy {
    padding:7px 12px 9px;
  }
}

.ie9 .four-per-row .coll-prod-buy {
  padding:6px 10px 6px;
}

.four-per-row .coll-prod-buy:hover, .three-per-row .coll-prod-buy:hover {
  background-color:{{ settings.global_button_color_hover }};
}

.four-per-row .coll-prod-meta {
  width:130px;
  float:left;
  margin-top:13px;
  font-size:13px;
}

.three-per-row .coll-prod-meta {
  width:191px;
  float:left;
  margin-top:19px;
}

.four-per-row .coll-prod-meta.no-medallion {
  width:191px;
  text-align:center;
}

.three-per-row .coll-prod-meta.no-medallion {
  width:268px;
  text-align:center;
}

.four-per-row .coll-prod-title {
  font-weight:700;
  font-family:{{ settings.text_font }};
}

.three-per-row .coll-prod-title {
  font-weight:700;
  font-size:16px;
  font-family:{{ settings.headings_font }};
}

.four-per-row .coll-prod-title:hover, .three-per-row .coll-prod-title:hover {
  color:{{ settings.global_hover_color }};
}

.four-per-row .coll-prod-price {
  color:{{ settings.price_color }};
  margin:3px 0 0 0;
}

.three-per-row .coll-prod-price {
  font-size:14px;
  color:{{ settings.price_color }};
  margin:6px 0 0 0;
}
                  
/* COLLECTION ITEMS */

.collection-list-product-count { line-height: 21px; margin: 0; }

/* INPUT */

label {
  display: block;
  font-size: 14px;
  color: #333;
  margin-bottom: 8px;           
}

input[type="checkbox"], input[type="radio"], .checkbox, .radio {
  margin:4px 5px 0 0;
  float:left;
}

.ie9 input[type="checkbox"], .ie9  input[type="radio"], .ie9  .checkbox, .ie9  .radio,
.lt-ie9 input[type="checkbox"], .lt-ie9 input[type="radio"], .lt-ie9 .checkbox, .lt-ie9 .radio {
