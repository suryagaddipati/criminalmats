/* SLIDESHOW */

#slideshow {
  padding:49px 0;
  border-bottom:1px solid {{ settings.primary_border }};
  text-align:center;
}

#slideshow-container {
  width:884px;
  {% assign height = settings.slideshow_height | remove: 'px' | times: 1 %}
  {% if height > 0 %}
  height:{{ height }}px;
  {% else %}
  height:490px;
  {% endif %}
  overflow:hidden;
  background-color:rgba(0,0,0,0.8);
  /* Addresses IE 6 + 7 overflow bug */
  *position:relative;
}

#slides {
  display:block;
  {% assign height = settings.slideshow_height | remove: 'px' | times: 1 %}
  {% if height > 0 %}
    height:{{ height }}px;
  {% else %}
    height:490px;
  {% endif %}
  min-width:884px;
  margin:0;
  padding:0;
}

.slide {
  width:884px;
  {% assign height = settings.slideshow_height | remove: 'px' | times: 1 %}
  {% if height > 0 %}
    height:{{ height }}px;
  {% else %}
    height:490px;
  {% endif %}
  display:block;
  float:left;
  background-color:rgba(255,255,255,0.9);
  position:relative;
}                   

.slide-link-through {
  position:absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  z-index:1337;
}

.slide-caption {
  position:absolute;
  bottom:0px;
  left:0px;
  width:880px;
  padding:25px 20px;
  background:rgba(0,0,0,0.5);
  color:#ffffff;
  font-family:{{ settings.headings_font }};
}

.lt-ie9 .slide-caption { background: url('{{ 'ie-caption-bg.png' | asset_url }}') repeat transparent; }

.slide-caption h2 {
  font-size:39px;
  line-height:1.3;
  margin:0;
  margin-left:-33px;
}

a.slide-link {
  font-size:20px;
  border-bottom:1px solid #ffffff;
  border-bottom:1px solid rgba(255,255,255,0.5);
  padding-bottom:1px;
  margin-top:16px;
  margin-bottom:10px;
  margin-left:-33px;
  color:#ffffff;
  display:inline-block;
  /* Necessary for inline-block in IE */
  zoom: 1;
  *display: inline;
}

#slideshow-controls {
  display:inline-block;
  list-style-type:none;
  margin:30px 0 0 0;
  padding:0;
  /* Necessary for inline-block in IE */
  zoom: 1;
  *display: inline;
}

#slideshow-controls li:first-child {
  margin-left:0;
}

.slide-control {
  float:left;
  display:block;
  height:9px;
  width:9px;
  color:{{ settings.slider_navigation }};
  padding:0 3px;
  cursor:pointer;
  font-size:24px;
  top:-7px \9;
  position:relative \9;
  font-size:38px \9;
}

.lt-ie9 .slide-control {
  top:-7px;
  position:relative;
  font-size:38px;
}

.slide-control.active {
  color:{{ settings.slider_navigation_hover }};
}

